    <style> 
      @font-face {
          font-family: 'CircularStd';
          src: url('{{ "CircularStd-Book_40e6560b-d983-4c75-9b34-fdbaf2bdeab2.woff2" | file_url }}') format('woff2');
          font-weight: normal;
          font-style: normal;
      }
      @font-face {
          font-family: 'CircularStd';
          src: url('{{ "CircularStd-Medium_62f2a405-ec30-4815-b197-57e76d4c4ab0.woff2" | file_url }}') format('woff2');
          font-weight: 500;
          font-style: normal;
      }
      * {
        font-family: 'CircularStd' !important;
      }
      h1, h2, h3, h4, h5, h6 {
          font-family: "CircularStd", sans-serif !important;
          color: #000 !important;
          font-weight: 500 !important;
      }

      .btn {
          font-weight: 500 !important;
          border-radius : 0 !important;
      }
      .js .sidebar .btn--disabled {
        background-color: #ccc !important;
      }
      .js .sidebar .btn--disabled .btn__content {
        color: #666 !important;
      }      
      .js .sidebar .btn--disabled span {
          color: #000 !important;
      }
      .btn .icon-svg {
          fill: #fff !important;
      } 
      div#catch-promo-earn-desktop {
