    {% else %}
      {{ 'lp-banner-split.min.css' | asset_url | stylesheet_tag }}
    {% endif %}

</section>

<style>
.lp-banner-split { opacity: 0; }
.banner-overlay {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
}
@media (max-width: 699px) {
	.banner-overlay {
		transform: translate(-50%,-50%) scale(0.9);
	}
}
.banner-overlay.mobile {
	display: block;
}
.banner-overlay.desktop {
	display: none;
} 
@media (min-width: 700px) {
	.banner-overlay.mobile {
		display: none;
	}
	.banner-overlay.desktop {
		display: block;
	}
}
img.banner-overlay[src*="sustainability-circle"] {
		animation: sustainability-rotate 20s infinite linear;
		transition: none;
}
@keyframes sustainability-rotate {
  from {transform: translate(-50%,-50%) scale(0.65) rotate(0deg);}
  to {transform: translate(-50%,-50%) scale(0.65) rotate(360deg);}
}
</style>

