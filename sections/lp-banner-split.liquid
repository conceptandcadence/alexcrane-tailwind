<section id="section-{{ section.id }}-{{ section.settings.variation }}" class="lp-section-variant{% if section.settings.visibility == 'mobile' %} visibility__mobile-only{% endif %}{% if section.settings.visibility == 'desktop' %} visibility__desktop-only{% endif %}" data-section-id="{{ section.id }}" data-variation="{{ section.settings.variation }}" data-title="{{ section.settings.title | escape }}">
    <div class="lp-banner-split">
        <div class="lp-banner-split__background">
            {% if section.settings.image_desktop %}
                {%- include 'image-size', sizes: '200,400,600,700,800,900,1000,1200', image: section.settings.image_desktop -%}
                {%- assign image_url_desktop = section.settings.image_desktop | img_url: '1x1' -%}
                <img class="lazyload-img banner-background desktop " loading="lazy"
                    src="{{ image_url_desktop }}" 
                    data-src="{{ image_url_desktop }}"
                    data-widths="[{{ supported_sizes }}]"
                    sizes="50vw"
                    srcset="{{ image_srcset }}"
                    alt="{{ section.settings.image_desktop.alt | default: page.title | escape }}">

                <noscript>
                    <img class="desktop" src="{{ section.settings.image_desktop | img_url: '1024x1024' }}" alt="{{ section.settings.image_desktop.alt | default: page.title | escape }}">
                </noscript>

                {% if section.settings.video_desktop_url != blank %}
                    <div class="banner-background banner-background__video desktop">
                        <video autoplay="" muted="" loop="" playsinline=""{% if section.settings.title != blank %} title="{{ section.settings.title | default: page.title | escape }}"{% endif %}{% if section.settings.image_desktop != blank %} poster="{{ section.settings.image_desktop | img_url: '600x' }}"{% endif %} class="banner-background__video-html lazy-video" style="position: absolute; top: 0; left: 0; width: 100%; height: 100%; transition: opacity 0.25s ease-in-out; opacity: 1;">
                            <source type="video/mp4" src="{{ section.settings.video_desktop_url }}">
                        </video>
                    </div>
                {% endif %}
            {% endif %}
 
            {% if section.settings.image_overlay_desktop %}
                {%- include 'image-size', sizes: '200,400,600,700,800,900,1000,1200', image: section.settings.image_overlay_desktop -%}
                {%- assign image_url_desktop = section.settings.image_overlay_desktop | img_url: '1x1' -%}
                <img class="lazyload-img banner-overlay desktop " loading="lazy"
                    src="{{ image_url_desktop }}" 
                    data-src="{{ image_url_desktop }}"
                    data-widths="[{{ supported_sizes }}]"
                    sizes="100vw"
                    srcset="{{ image_srcset }}"
                    alt="{{ section.settings.image_overlay_desktop.alt | escape }}">
            {% endif %}

            {% if section.settings.image_mobile %}
                {%- include 'image-size', sizes: '200,400,600,700,800,900,1000,1200', image: section.settings.image_mobile -%}
                {%- assign image_url_desktop  = section.settings.image_mobile | img_url: '1x1' -%}
                <img class="lazyload-img banner-background mobile " loading="lazy"
                    src="{{ image_url_desktop }}" 
                    data-src="{{ image_url_desktop }}"
                    data-widths="[{{ supported_sizes }}]"
                    sizes="100vw"
                    srcset="{{ image_srcset }}"
                    alt="{{ section.settings.image_mobile.alt | default: page.title | escape }}">

                <noscript>
                    <img class="mobile" src="{{ section.settings.image_mobile | img_url: '1024x1024' }}" alt="{{ section.settings.image_mobile.alt | default: page.title | escape }}">
                </noscript>
                
                {% if section.settings.video_mobile_url != blank %}
                    <div class="banner-background banner-background__video mobile">
                        <video autoplay="" muted="" loop="" playsinline=""{% if section.settings.title != blank %} title="{{ section.settings.title | default: page.title | escape }}"{% endif %}{% if section.settings.image_mobile != blank %} poster="{{ section.settings.image_mobile | img_url: '600x' }}"{% endif %} class="banner-background__video-html lazy-video" style="position: absolute; top: 0; left: 0; width: 100%; height: 100%; transition: opacity 0.25s ease-in-out; opacity: 1;">
                            <source type="video/mp4" src="{{ section.settings.video_mobile_url }}">
                        </video>
                    </div>
                {% endif %}
            {% endif %}

            {% if section.settings.image_overlay_mobile %}
                {%- include 'image-size', sizes: '200,400,600,700,800,900,1000,1200', image: section.settings.image_overlay_mobile -%}
                {%- assign image_url_mobile = section.settings.image_overlay_mobile | img_url: '1x1' -%}
                <img class="lazyload-img banner-overlay mobile " loading="lazy"
                    src="{{ image_url_mobile }}" 
                    data-src="{{ image_url_mobile }}"
                    data-widths="[{{ supported_sizes }}]"
                    sizes="100vw"
                    srcset="{{ image_srcset }}"
                    alt="{{ section.settings.image_overlay_mobile.alt | escape }}">
            {% endif %}

            {% comment %}
            <style>
                #shopify-section-{{ section.id }} .lp-banner-split__background {
                    background-color: {{ section.settings.background_color_mobile }};
                }
                @media (min-width: 700px) {
                    #shopify-section-{{ section.id }} .lp-banner-split__background,
                    #shopify-section-{{ section.id }} .lp-banner-split__foreground {
                        background-color: {{ section.settings.background_color_desktop }};
                    }
                }
            </style>
            {% endcomment %}
        </div>
        <div class="lp-banner-split__foreground-wrapper">
          <div class="lp-banner-split__foreground">
            {% for block in section.blocks %}
                {% case block.type %}
                    {% when 'image' %}
                        {% if block.settings.image != blank %}

                            {% assign media_intrinsic_width = block.settings.width_m | default: block.settings.max_width_m | default: '100px' %}
                            {% assign media_intrinsic_height = block.settings.height_m | default: block.settings.max_height_m | default: '100px' %}

                            {% if block.settings.width != blank or block.settings.height != blank %}
                                {% capture image_dimensions %}{{ block.settings.width }}x{{ block.settings.height }}{% endcapture %}
                            {% else %}
                                {% assign image_dimensions = '1024x1024' %}
                            {% endif %}
                            {%- assign image_url_mobile = block.settings.image | img_url: image_dimensions -%}
                            <img id="banner-item-{{ block.id }}" loading="lazy" width="{{ media_intrinsic_width }}" class="banner-item image lazyload-img  {% if block.settings.visibility == 'mobile' %}visibility__mobile-only{% endif %}{% if block.settings.visibility == 'desktop' %}visibility__desktop-only{% endif %}"
                                src="{{ image_url_mobile }}" 
                                data-src="{{ image_url_mobile }}"
                                sizes="100vw"
                                alt="{{ section.settings.image.alt | default: page.title | escape }}">
                            <noscript>
                                <img class="mobile" src="{{ image_url_mobile }}" alt="{{ section.settings.image.alt | default: page.title | escape }}">
                            </noscript>
                            <style>
                                {% if block.settings.width_m != blank %} #banner-item-{{ block.id }} { width: {{ block.settings.width_m }}; } {% endif %}
                                {% if block.settings.min_width_m != blank %} #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_m }}; } {% endif %}
                                {% if block.settings.max_width_m != blank %} #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_m | default: '100%' }}; } {% endif %}
                                {% if block.settings.height_m != blank %} #banner-item-{{ block.id }} { height: {{ block.settings.height_m }}; } {% endif %}
                                {% if block.settings.min_height_m != blank %} #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_m }}; } {% endif %}
                                {% if block.settings.max_height_m != blank %} #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_m }}; } {% endif %}
                                {% if block.settings.width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { width: {{ block.settings.width_d }}; } } {% endif %}
                                {% if block.settings.min_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_d }}; } } {% endif %}
                                {% if block.settings.max_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_d | default: '100%' }}; } } {% endif %}
                                {% if block.settings.height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { height: {{ block.settings.height_d }}; } } {% endif %}
                                {% if block.settings.min_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_d }}; } } {% endif %}
                                {% if block.settings.max_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_d }}; } } {% endif %}
                                #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_m }}px {{ block.settings.padding_right_m }}px {{ block.settings.padding_bottom_m }}px {{ block.settings.padding_left_m }}px; }
                                @media (min-width: 700px) { #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_d }}px {{ block.settings.padding_right_d }}px {{ block.settings.padding_bottom_d }}px {{ block.settings.padding_left_d }}px; } }
                            </style>
                        {% endif %}      
                    {% when 'svg' %}
                        {% if block.settings.image != blank %}

                            {% assign media_intrinsic_width = block.settings.width_m | default: block.settings.max_width_m | default: '100px' %}
                            {% assign media_intrinsic_height = block.settings.height_m | default: block.settings.max_height_m | default: '100px' %}

                            <img id="banner-item-{{ block.id }}" class="banner-item svg {% if block.settings.visibility == 'mobile' %}visibility__mobile-only{% endif %}{% if block.settings.visibility == 'desktop' %}visibility__desktop-only{% endif %}" width="{{ media_intrinsic_width }}" height="{{ media_intrinsic_height }}" src="{{ block.settings.image }}" alt="{{ section.settings.image.alt | default: page.title | escape }}">
                            <style>
                                {% if block.settings.width_m != blank %} #banner-item-{{ block.id }} { width: {{ block.settings.width_m }}; } {% endif %}
                                {% if block.settings.min_width_m != blank %} #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_m }}; } {% endif %}
                                {% if block.settings.max_width_m != blank %} #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_m | default: '100%' }}; } {% endif %}
                                {% if block.settings.height_m != blank %} #banner-item-{{ block.id }} { height: {{ block.settings.height_m }}; } {% endif %}
                                {% if block.settings.min_height_m != blank %} #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_m }}; } {% endif %}
                                {% if block.settings.max_height_m != blank %} #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_m }}; } {% endif %}
                                {% if block.settings.width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { width: {{ block.settings.width_d }}; } } {% endif %}
                                {% if block.settings.min_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_d }}; } } {% endif %}
                                {% if block.settings.max_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_d | default: '100%' }}; } } {% endif %}
                                {% if block.settings.height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { height: {{ block.settings.height_d }}; } } {% endif %}
                                {% if block.settings.min_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_d }}; } } {% endif %}
                                {% if block.settings.max_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_d }}; } } {% endif %}
                                #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_m }}px {{ block.settings.padding_right_m }}px {{ block.settings.padding_bottom_m }}px {{ block.settings.padding_left_m }}px; }
                                @media (min-width: 700px) { #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_d }}px {{ block.settings.padding_right_d }}px {{ block.settings.padding_bottom_d }}px {{ block.settings.padding_left_d }}px; } }
                            </style>
                        {% endif %}          
                    {% when 'button' %}
                        {% if block.settings.button_text != blank %}
                            <a id="banner-item-{{ block.id }}" class="banner-item button {{ block.settings.button_style }} {% if block.settings.visibility == 'mobile' %}visibility__mobile-only{% endif %}{% if block.settings.visibility == 'desktop' %}visibility__desktop-only{% endif %}" href="{{ block.settings.button_url | default: '#' }}">{{ block.settings.button_text }}</a>
                            <style>
                                {% if block.settings.width_m != blank %} #banner-item-{{ block.id }} { width: {{ block.settings.width_m }}; } {% endif %}
                                {% if block.settings.min_width_m != blank %} #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_m }}; } {% endif %}
                                {% if block.settings.max_width_m != blank %} #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_m | default: '100%' }}; } {% endif %}
                                {% if block.settings.height_m != blank %} #banner-item-{{ block.id }} { height: {{ block.settings.height_m }}; } {% endif %}
                                {% if block.settings.min_height_m != blank %} #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_m }}; } {% endif %}
                                {% if block.settings.max_height_m != blank %} #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_m }}; } {% endif %}
                                {% if block.settings.width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { width: {{ block.settings.width_d }}; } } {% endif %}
                                {% if block.settings.min_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_d }}; } } {% endif %}
                                {% if block.settings.max_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_d | default: '100%' }}; } } {% endif %}
                                {% if block.settings.height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { height: {{ block.settings.height_d }}; } } {% endif %}
                                {% if block.settings.min_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_d }}; } } {% endif %}
                                {% if block.settings.max_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_d }}; } } {% endif %}
                                #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_m }}px {{ block.settings.padding_right_m }}px {{ block.settings.padding_bottom_m }}px {{ block.settings.padding_left_m }}px; }
                                @media (min-width: 700px) { #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_d }}px {{ block.settings.padding_right_d }}px {{ block.settings.padding_bottom_d }}px {{ block.settings.padding_left_d }}px; } }
                            </style>
                        {% endif %}  
                    {% when 'text' %}
                        {% if block.settings.title != blank %}
                            {%- capture opening_tag -%}<{{ block.settings.text_style }}>{%- endcapture -%}
                            {%- capture closing_tag -%}</{{ block.settings.text_style }}>{%- endcapture -%} 
                            <div id="banner-item-{{ block.id }}" class="banner-item richtext {{ block.settings.button_style }} {% if block.settings.visibility == 'mobile' %}visibility__mobile-only{% endif %}{% if block.settings.visibility == 'desktop' %}visibility__desktop-only{% endif %}" style="{% if block.settings.width != blank %}width: {{ block.settings.width }};{% endif %}{% if block.settings.height != blank %}height: {{ block.settings.height }};{% endif %}">
                                {{ block.settings.title | replace: '<p>', opening_tag | replace: '</p>', closing_tag }}
                            </div>
                            <style>
                                #banner-item-{{ block.id }} * { text-align: {{ block.settings.alignment }}; color: {{ block.settings.color }}; }
                                {% if block.settings.width_m != blank %} #banner-item-{{ block.id }} { width: {{ block.settings.width_m }}; } {% endif %}
                                {% if block.settings.min_width_m != blank %} #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_m }}; } {% endif %}
                                {% if block.settings.max_width_m != blank %} #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_m | default: '100%' }}; } {% endif %}
                                {% if block.settings.height_m != blank %} #banner-item-{{ block.id }} { height: {{ block.settings.height_m }}; } {% endif %}
                                {% if block.settings.min_height_m != blank %} #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_m }}; } {% endif %}
                                {% if block.settings.max_height_m != blank %} #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_m }}; } {% endif %}
                                {% if block.settings.width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { width: {{ block.settings.width_d }}; } } {% endif %}
                                {% if block.settings.min_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_d }}; } } {% endif %}
                                {% if block.settings.max_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_d | default: '100%' }}; } } {% endif %}
                                {% if block.settings.height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { height: {{ block.settings.height_d }}; } } {% endif %}
                                {% if block.settings.min_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_d }}; } } {% endif %}
                                {% if block.settings.max_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_d }}; } } {% endif %}
                                #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_m }}px {{ block.settings.padding_right_m }}px {{ block.settings.padding_bottom_m }}px {{ block.settings.padding_left_m }}px; }
                                @media (min-width: 700px) { #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_d }}px {{ block.settings.padding_right_d }}px {{ block.settings.padding_bottom_d }}px {{ block.settings.padding_left_d }}px; } }
                            </style>
                        {% endif %}  
                    {% when 'spacer' %}
                        <div class="banner-item spacer {% if block.settings.visibility == 'mobile' %}visibility__mobile-only{% endif %}{% if block.settings.visibility == 'desktop' %}visibility__desktop-only{% endif %}" style="{% if block.settings.width != blank %}width: {{ block.settings.width }};{% endif %}{% if block.settings.height != blank %}height: {{ block.settings.height }};{% endif %}"></div>
                        <style>
                            {% if block.settings.width_m != blank %} #banner-item-{{ block.id }} { width: {{ block.settings.width_m }}; } {% endif %}
                            {% if block.settings.min_width_m != blank %} #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_m }}; } {% endif %}
                            {% if block.settings.max_width_m != blank %} #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_m | default: '100%' }}; } {% endif %}
                            {% if block.settings.height_m != blank %} #banner-item-{{ block.id }} { height: {{ block.settings.height_m }}; } {% endif %}
                            {% if block.settings.min_height_m != blank %} #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_m }}; } {% endif %}
                            {% if block.settings.max_height_m != blank %} #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_m }}; } {% endif %}
                            {% if block.settings.width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { width: {{ block.settings.width_d }}; } } {% endif %}
                            {% if block.settings.min_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-width: {{ block.settings.min_width_d }}; } } {% endif %}
                            {% if block.settings.max_width_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-width: {{ block.settings.max_width_d | default: '100%' }}; } } {% endif %}
                            {% if block.settings.height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { height: {{ block.settings.height_d }}; } } {% endif %}
                            {% if block.settings.min_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { min-height: {{ block.settings.min_height_d }}; } } {% endif %}
                            {% if block.settings.max_height_d != blank %} @media (min-width: 700px) { #banner-item-{{ block.id }} { max-height: {{ block.settings.max_height_d }}; } } {% endif %}
                            #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_m }}px {{ block.settings.padding_right_m }}px {{ block.settings.padding_bottom_m }}px {{ block.settings.padding_left_m }}px; }
                            @media (min-width: 700px) { #banner-item-{{ block.id }} { margin: {{ block.settings.padding_top_d }}px {{ block.settings.padding_right_d }}px {{ block.settings.padding_bottom_d }}px {{ block.settings.padding_left_d }}px; } }
                        </style>        
                    {% endcase %}
            {% endfor %}
          </div>
        </div>
        <style>

            @media (max-width: 699px) {
                #shopify-section-{{ section.id }} {
                    padding: {{ section.settings.padding_top_m }}px {{ section.settings.padding_right_m }}px {{ section.settings.padding_bottom_m }}px {{ section.settings.padding_left_m }}px;
                }
                #shopify-section-{{ section.id }} .lp-banner-split__foreground {
                    flex-direction: {{ section.settings.alignment_direction_m }};
                    justify-content: {{ section.settings.alignment_horizontal_m }};
                    align-items: {{ section.settings.alignment_vertical_m }};
                    padding: {{ section.settings.inner_padding_top_m }}px {{ section.settings.inner_padding_right_m }}px {{ section.settings.inner_padding_bottom_m }}px {{ section.settings.inner_padding_left_m }}px;
                }
            }
            @media (min-width: 700px) {
                #shopify-section-{{ section.id }} {
                    padding: {{ section.settings.padding_top_d }}px {{ section.settings.padding_right_d }}px {{ section.settings.padding_bottom_d }}px {{ section.settings.padding_left_d }}px;
                }
                #shopify-section-{{ section.id }} .lp-banner-split__foreground {
                    flex-direction: {{ section.settings.alignment_direction_d }};
                    justify-content: {{ section.settings.alignment_horizontal_d }};
                    align-items: {{ section.settings.alignment_vertical_d }};
                    padding: {{ section.settings.inner_padding_top_d }}px {{ section.settings.inner_padding_right_d }}px {{ section.settings.inner_padding_bottom_d }}px {{ section.settings.inner_padding_left_d }}px;
                }
            }
            {% if section.settings.image_mobile == blank and section.settings.video_mobile_url == blank %}
                @media (max-width: 699px) {
                    #shopify-section-{{ section.id }} .lp-banner-split__foreground { position: relative !important; }
                    #shopify-section-{{ section.id }} .lp-banner-split__foreground-wrapper { padding-bottom: {{ 1.00 | divided_by: aspect_ratio_mobile | times: 100 }}%; }
                }
            {% endif %}
            {% if section.settings.image_desktop == blank and section.settings.video_desktop_url == blank %}
                @media (min-width: 700px) {
                    #shopify-section-{{ section.id }} .lp-banner-split__foreground { position: relative !important; }
                    #shopify-section-{{ section.id }} .lp-banner-split__foreground-wrapper { padding-bottom: {{ 1.00 | divided_by: aspect_ratio_desktop | times: 100 }}%; }
                }
            {% endif %}
        </style>
    </div>

    {% if section.settings.marquee %}
      <div class="marquee" style="padding-top: {{ section.settings.marquee_padding }}px; padding-bottom: {{ section.settings.marquee_padding }}px;">
          <div class="marquee__inner" aria-hidden="true">
              <img src="{{ 'sustainability-line.svg' | asset_url }}" alt="" />
              <img src="{{ 'sustainability-line.svg' | asset_url }}" alt="" />
          </div>
      </div>
      <style>
      	.marquee {
      		position: relative;
      		overflow: hidden;
      		width: 100%;
      		left: 0;
            max-width: 100%;
      		padding-bottom: 48px;
          --offset: calc(100vw - 14px);
          --move-initial: calc(-50% + var(--offset));
          --move-final: calc(50% + var(--offset));
      	}
      	@media (min-width: 700px) {
      		.marquee {
      			display: none !important;
      		}
      	}
      	.marquee__inner {
      			width: 900px;
      			display: flex;
      			position: relative;
      			transform: translate3d(var(--move-initial), 0, 0);
      			animation: marquee 15s linear infinite;
      			animation-play-state: running;
      	}
      
      	.marquee svg, .marquee img {
          height: 24px;
          padding: 0 16px;
      	}
      
      	@keyframes marquee {
      			0% {
      					transform: translate3d(0%, 0, 0);
      			}
      
      			100% {
      					transform: translate3d(-100%, 0, 0);
      			}
      	}
      </style>
    {% endif %}
